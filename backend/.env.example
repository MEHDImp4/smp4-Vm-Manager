# Server Configuration
PORT=3000

# Proxmox API Configuration
PROXMOX_URL=https://your-proxmox-ip:8006
# Format: USER@REALM!TOKENID=UUID
PROXMOX_API_TOKEN=root@pam!tokenid=uuid
